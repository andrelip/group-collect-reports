<%= form_for @conn, Routes.import_path(@conn, :post), [multipart: true], fn f -> %>
  <div class="form-group">
    <label>Please insert the input CSV to load the report data.</label>
    <%= file_input f, :csv, class: "form-control-file" %>
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
<% end %>
